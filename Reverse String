class Solution(object):
    def reverseString(self, s):
        i = 0
        j = len(s) - 1
        while i<j:
          s[i],s[j] = s[j],s[i]
          i+=1
          j-=1
        print(s)
__________________________________________

#include <string.h>
void reverseString(char* s, int sSize) {
    int i = 0;
    int j = sSize - 1;
    while (i < j) {
        char temp = s[i];
        s[i] = s[j];
        s[j] = temp;
        i++;
        j--;
    }
}
