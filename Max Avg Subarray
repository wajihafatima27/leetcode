class Solution:
    def findMaxAverage(self, nums: List[int], k: int) -> float:
        m=sum(nums[0:k])
        max=m
        i=0
        j=k-1
        while j<len(nums)-1:
            m=m-nums[i]+nums[j+1]
            i+=1
            j+=1
            if m>max:
                max=m
        return max/k
